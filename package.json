  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "yarn start:cxfi:dev",
    "next": "yarn start:cxfi:next",
    "build": "ng build",
    "test": "ng test",
    "test:all": "ng run cxfi:test-all",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "reset": "rm -rf functions/node_modules && rm -rf dist node_modules && yarn && yarn --cwd functions install",
    "commit": "yarn format && git add -A && git-cz",
    "commit:staged": "yarn format && git-cz",
    "start:cxfi:dev": "ng serve",
    "start:cxfi:next": "ng serve --configuration=next",
    "build:cxfi:dev": "ng build",
    "build:cxfi:next": "ng build --configuration=next",
    "build:cxfi:stg": "ng build --configuration=stg",
    "build:cxfi:prod": "ng build --configuration=production",
    "test:ci": "echo 'skipping tests'",
    "//": "OLD TEST:CI ----- ng test --watch=false --single-run=true --browser=ChromeHeadless",
    "predeploy": "firebase use $CI_ENVIRONMENT_SLUG --token $FIREBASE_TOKEN && firebase functions:config:get",
    "deploy": "firebase deploy --non-interactive --token $FIREBASE_TOKEN",
    "release": "./node_modules/.bin/semantic-release-gitlab",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
